<launch>
    <rosparam command="load" file="$(find hdt)/config/roman_control.yaml"/>

    <node name="controller_spawner" pkg="controller_manager" type="spawner"
            args="joint_state_controller
                    limb_left_joint1_controller
                    limb_left_joint2_controller
                    limb_left_joint3_controller
                    limb_left_joint4_controller
                    limb_left_joint5_controller
                    limb_left_joint6_controller
                    limb_left_joint7_controller
                    limb_right_joint1_controller
                    limb_right_joint2_controller
                    limb_right_joint3_controller
                    limb_right_joint4_controller
                    limb_right_joint5_controller
                    limb_right_joint6_controller
                    limb_right_joint7_controller
                    torso_joint1_controller"/>

    <node name="pub_torso_joint1" pkg="rostopic" type="rostopic" args="pub -r 50 torso_joint1_controller/command std_msgs/Float64 0.0"/>

    <node name="pub_limb_left_joint1" pkg="rostopic" type="rostopic" args="pub -r 50 limb_left_joint1_controller/command std_msgs/Float64 0.0"/>
    <node name="pub_limb_left_joint2" pkg="rostopic" type="rostopic" args="pub -r 50 limb_left_joint2_controller/command std_msgs/Float64 0.0"/>
    <node name="pub_limb_left_joint3" pkg="rostopic" type="rostopic" args="pub -r 50 limb_left_joint3_controller/command std_msgs/Float64 0.0"/>
    <node name="pub_limb_left_joint4" pkg="rostopic" type="rostopic" args="pub -r 50 limb_left_joint4_controller/command std_msgs/Float64 0.0"/>
    <node name="pub_limb_left_joint5" pkg="rostopic" type="rostopic" args="pub -r 50 limb_left_joint5_controller/command std_msgs/Float64 0.0"/>
    <node name="pub_limb_left_joint6" pkg="rostopic" type="rostopic" args="pub -r 50 limb_left_joint6_controller/command std_msgs/Float64 0.0"/>
    <node name="pub_limb_left_joint7" pkg="rostopic" type="rostopic" args="pub -r 50 limb_left_joint7_controller/command std_msgs/Float64 0.0"/>

    <node name="pub_limb_right_joint1" pkg="rostopic" type="rostopic" args="pub -r 50 limb_right_joint1_controller/command std_msgs/Float64 0.0"/>
    <node name="pub_limb_right_joint2" pkg="rostopic" type="rostopic" args="pub -r 50 limb_right_joint2_controller/command std_msgs/Float64 0.0"/>
    <node name="pub_limb_right_joint3" pkg="rostopic" type="rostopic" args="pub -r 50 limb_right_joint3_controller/command std_msgs/Float64 0.0"/>
    <node name="pub_limb_right_joint4" pkg="rostopic" type="rostopic" args="pub -r 50 limb_right_joint4_controller/command std_msgs/Float64 0.0"/>
    <node name="pub_limb_right_joint5" pkg="rostopic" type="rostopic" args="pub -r 50 limb_right_joint5_controller/command std_msgs/Float64 0.0"/>
    <node name="pub_limb_right_joint6" pkg="rostopic" type="rostopic" args="pub -r 50 limb_right_joint6_controller/command std_msgs/Float64 0.0"/>
    <node name="pub_limb_right_joint7" pkg="rostopic" type="rostopic" args="pub -r 50 limb_right_joint7_controller/command std_msgs/Float64 0.0"/>


    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>
</launch>
