source screenrc.common

screen -t core
stuff "roscore^M"

screen -t rviz
stuff "rosrun rviz rviz -d $ROS_WORKSPACE/config/rcta.rviz"

screen -t ctrl
stuff "roslaunch rcta roman_control_ns.launch live:=false fake:=true --screen"

screen -t pose
stuff "roslaunch rcta roman_pose_ns.launch live:=false fake:=true --screen"

screen -t repo
stuff "roslaunch rcta reposition_base_executor.launch --screen"

screen -t move
stuff "roslaunch rcta move_arm_node_ns.launch live:=false fake:=true --screen"

screen -t grasp
stuff "roslaunch rcta grasp_object_executor.launch --screen"
