<launch>

  <!-- This file makes it easy to include the settings for sensor managers -->  

  <!-- Load the robot specific sensor manager; this sets the moveit_sensor_manager ROS parameter -->
  <arg name="moveit_sensor_manager" default="hdt_arm" />
  <include file="$(find hdt_moveit_config)/launch/$(arg moveit_sensor_manager)_moveit_sensor_manager.launch" />

  <!-- TODO: Check whether these need to be here in Groovy and what effect they have on the octomap -->
  <param name="octomap_frame" type="string" value="camera_link" />
  <param name="octomap_resolution" type="double" value="0.05" />
  <param name="max_range" type="double" value="3.5" />
</launch>
